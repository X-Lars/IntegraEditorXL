<UserControl x:Class="IntegraEditorXL.UserControls.SNA.SNAParametersStrings2"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:IntegraEditorXL.UserControls.Components"
             xmlns:models="clr-namespace:IntegraXL.Models.Parameters;assembly=IntegraXL"
             xmlns:xl="clr-namespace:ControlsXL;assembly=ControlsXL"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Background="White"
             d:DataContext="{d:DesignInstance Type=models:SNAStrings3}">

    <StackPanel DataContext="{Binding Parameters}">

        <xl:EnumeratedText Prefix="Hold Legato Mode (CC19)"
                           Value="{Binding HoldLegatoMode}"/>
       
        <xl:EnumeratedText Prefix="Variation"
                           Value="{Binding Variation}"/>

    </StackPanel>
</UserControl>